/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;


.center {
  @apply min-h-screen flex items-center justify-center;
}

h1 { @apply h1}
h2 { @apply h2}
h3 { @apply h3}
h4 { @apply h4}
h5 { @apply h5}
h6 { @apply h6}

.invalid {
  @apply variant-filled-error;
}